 /* Safari 5.1, Chrome 10+ */ background: -webkit-linear-gradient(top, #000066, #3366CC); 
 /* Firefox 3.6+ */ background: -moz-linear-gradient(top, #000066, #3366CC); 
 /* IE 10 */ background: -ms-linear-gradient(top, #000066, #3366CC); 
 /* Opera 11.10+ */ background: -o-linear-gradient(top, #000066, #3366CC);
}
#nav li a.top_link span {
float:left;
display:block;
padding:0 24px 0 12px;
